{
    "manifest_version": 3,
    "name": "Terraria Redirect",
    "version": "2.0",
    "description": "Automatically redirects from icky fandom to wiki.gg",
    "permissions": [
      "webNavigation",
      "activeTab"
    ],
    "host_permissions": [
      "http://terraria.fandom.com/*",
      "https://terraria.fandom.com/*",
      "http://thoriummod.fandom.com/*",
      "https://thoriummod.fandom.com/*"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    },
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }
  